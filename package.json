{
  "name": "mantine-next-nextra-template",
  "version": "2.3.2",
  "private": true,
  "scripts": {
    "generate-data": "node scripts/generate_carousel_data.js",
    "dev": "npm run generate-data && next dev --turbopack",
    "prebuild": "node scripts/generate_carousel_data.js",
    "build": "npm run generate-data && next build && yarn build:pagefind",
    "build:pagefind": "pagefind --site .next/server/app --output-path public/_pagefind",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "next lint",
    "stylelint": "stylelint '**/*.css' --cache",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "npm run prettier:check && npm run lint && npm run typecheck && npm run jest",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "release:patch": "yarn version patch",
    "release:minor": "yarn version minor",
    "release:major": "yarn version major"
  },
  "dependencies": {
    "@gfazioli/mantine-clock": "^2.1.3",
    "@gfazioli/mantine-marquee": "^2.6.2",
    "@gfazioli/mantine-text-animate": "^2.3.2",
    "@mantine/carousel": "^8.3.5",
    "@mantine/core": "8.3.5",
    "@mantine/hooks": "8.3.5",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/bundle-analyzer": "^15.5.6",
    "@tabler/icons-react": "^3.35.0",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.546.0",
    "next": "^16.0.0",
    "next-mdx-remote": "^5.0.0",
    "nextra": "4.6.0",
    "nextra-theme-docs": "4.6.0",
    "nodemailer": "^7.0.9",
    "next-mdx-rs": "^0.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "recharts": "^3.3.0",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@eslint/js": "^9.38.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@storybook/nextjs": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.12",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "19.2.2",
    "babel-loader": "^10.0.0",
    "eslint": "^9.38.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "gray-matter": "^4.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pagefind": "^1.4.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-preset-mantine": "1.18.0",
